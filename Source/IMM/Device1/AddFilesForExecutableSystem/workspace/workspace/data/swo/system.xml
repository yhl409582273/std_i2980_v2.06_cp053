<?xml version="1.0"?>

<system>
  <EVENT name="Alarm->Switch to manual">system.evAlarmAutoManual</EVENT>
  <EVENT name="Alarm->Finish cycle and switch to manual">system.evAlarmFinishCycleAndStop</EVENT>

  <EVENT name="Alarm->Motor lock">system.evAlarmMotorLock</EVENT>
  <EVENT name="Alarm->Motor unlock">system.evAlarmMotorRelease</EVENT>

  <EVENT name="Alarm->Lamp blink">system.evAlarmLampBlink</EVENT>
  <EVENT name="Alarm->Lamp reset">system.evAlarmLampReset</EVENT>

  <EVENT name="Watchdog">system.evAlarmWatchdog</EVENT>

  <EVENT name="Movement start">system.evStart</EVENT>
  <EVENT name="Movement stop">system.evStop</EVENT>
  <EVENT name="Movement start request">system.evStartRequest</EVENT>
  <EVENT name="Movement stop request">system.evStopRequest</EVENT>
</system>

